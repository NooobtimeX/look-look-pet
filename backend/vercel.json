{
    "version": 2,
    "env": {
        "MONGO_URI": "localhost:27017/looklookdb",
        "JWT_SECRET":"yourSecretKey",
        "SESSION_SECRET":"yourSecretFromEnv",
        "CLIENT_URL":"http://localhost:3000",
        "PORT":"3001",
        "BCRYPT_SALT_ROUNDS":"10"
    },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}